#include <stdlib.h>
#include <stdio.h>
#include <iostream>

int main (){
	int num1;
	int num2;
	
	printf("Digite um número:");
	scanf("%i", &num1);
	printf("Digite um número diferente do anterior:");
	scanf("%i", &num2);
	
	float divisao = num1 / num2; 
	printf("A divisao entre os dois números é: %f", divisao);
	
	return 0;

}
	
