#include <stdlib.h>
#include <stdio.h>
#include <iostream>

int main (){
	float num1;
	float num2;
	
	printf("Digite um numero:");
	scanf("%f", &num1);
	printf("Digite um numero diferente do anterior:");
	scanf("%f", &num2);
	
	float diferenca = num1 - num2; 
	printf("A diferença entre os dois numeros é: %f", diferenca);
	
	return 0;

}
