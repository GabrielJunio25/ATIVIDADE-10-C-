#include <stdio.h>

int main() {
    int a, b, temp;

    printf("Digite o valor de A: ");
    scanf("%d", &a);

    printf("Digite o valor de B: ");
    scanf("%d", &b);

    temp = a;
    a = b;
    b = temp;

    printf("\nO valor antigo de A era: %d", a);
    printf(" e o novo valor e: %d\n", a);

    printf("O valor antigo de B era: %d", b);
    printf(" e o novo valor e: %d\n", b);

    return 0;
}
